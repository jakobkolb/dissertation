issn = {1474-547X},
journal = {Lancet},
keywords = {Agriculture,Agriculture: economics,Agriculture: statistics {\&} numerical data,Agriculture: trends,Animals,Dairy Products,Dairy Products: supply {\&} distribution,Dairy Products: utilization,Developed Countries,Developed Countries: economics,Developed Countries: statistics {\&} numerical data,Energy Metabolism,Food Supply,Food Supply: economics,Food Supply: statistics {\&} numerical data,Greenhouse Effect,Humans,Meat,Meat: economics,Meat: supply {\&} distribution,Meat: utilization,climate change,metabolism},
mendeley-tags = {climate change,metabolism},
month = {oct},
number = {9594},
pages = {1253--63},
pmid = {17868818},
title = {{Food, livestock production, energy, climate change, and health.}},
