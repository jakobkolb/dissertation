issn = {0066-426X},
journal = {Annu. Rev. Phys. Chem.},
keywords = {Amino Acid,Amino Acid Sequence,Computer Simulation,Models,Molecular,Molecular Sequence Data,Polymers,Protein Folding,Sequence Homology,Thermodynamics},
