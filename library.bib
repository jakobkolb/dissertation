title = {{Sources and Consequences of Knowledge Depreciation}},
url = {http://www.iiasa.ac.at/web/home/research/researchPrograms/TransitionstoNewTechnologies/05{\_}Grubler{\_}Knowledge{\_}Depreciation{\_}WEB.pdf},
year = {2013}
}
@article{Pachur2013,
abstract = {How does expertise impact the selection of decision strategies? We asked airport customs officers and a novice control group to decide which passengers (described on several cue dimensions) they would submit to a search. Additionally, participants estimated the validities of the different cues. Then we modeled the decisions using compensatory strategies, which integrate many cues, and a noncompensatory heuristic, which relies on one-reason decision making. The majority of the customs officers were best described by the noncompensatory heuristic, whereas the majority of the novices were best described by a compensatory strategy. We also found that the experts' subjective cue validity estimates showed a higher dispersion across the cues and that differences in cue dispersion partially mediated differences in strategy use between experts and novices. Our results suggest that experts often rely on one-reason decision making and that expert-novice differences in strategy selection may reflect a response to the internal representation of the environment. ?? 2013 Elsevier B.V.},
author = {Pachur, Thorsten and Marinello, Gianmarco},
doi = {10.1016/j.actpsy.2013.05.003},
file = {:home/jakob/Mendeley{\_}Papers/Pachur, Marinello/Pachur, Marinello - 2013 - Expert intuitions How to model the decision strategies of airport customs officers.pdf:pdf},
issn = {00016918},
journal = {Acta Psychol. (Amst).},
keywords = {Customs,Decision strategies,Expertise,Heuristics,Strategy selection,heuristic decision-making},
