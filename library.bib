journal = {Ecol. Modell.},
keywords = {Agent-based model,Bounded rationality,Human decision-making,Human-wildlife interaction,Pattern-oriented modelling,Whale-watching,agent-based modelling,boundet rationality,heuristic decision-making},
mendeley-tags = {agent-based modelling,boundet rationality,heuristic decision-making},
number = {14},
