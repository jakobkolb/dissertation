isbn = {1195-5449},
issn = {1708-3087},
journal = {Ecol. Soc.},
number = {2},
pages = {5},
pmid = {18129228},
title = {{Resilience, Adaptability and Transformability in Social-ecological Systems}},
url = {http://www.ecologyandsociety.org/vol9/iss2/art5/},
volume = {9},
year = {2004}
}
