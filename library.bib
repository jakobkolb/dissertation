issn = {0027-8424},
journal = {Proc. Natl. Acad. Sci.},
number = {35},
pages = {13908--13914},
pmid = {22912403},
title = {{Classic Period collapse of the Central Maya Lowlands: Insights about human-environment relationships for sustainability}},
