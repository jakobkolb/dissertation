doi = {10.1038/nature03289},
isbn = {0028-0836},
issn = {1476-4687},
journal = {Nature},
keywords = {Africa,Algorithms,Animals,Biodiversity,Conservation of Natural Resources,Conservation of Natural Resources: methods,Data Collection,Ecology,Ecology: methods,Geography,International Cooperation,Mammals,Mammals: physiology,Plant Physiological Phenomena,Population Dynamics,Sensitivity and Specificity,Southern,Time Factors},
