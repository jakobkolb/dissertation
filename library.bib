journal = {J. R. Soc. Interface},
keywords = {Animals,Biological,Communicable Diseases,Computer Simulation,Disease Outbreaks,Disease Outbreaks: statistics {\&} numerical data,Disease Susceptibility,Disease Susceptibility: epidemiology,Disease Transmission,Emerging,Emerging: epidemiology,Epidemiologic Methods,Humans,Incidence,Infectious,Infectious: statistics {\&} num,Models,Population Dynamics,Prevalence,Risk Assessment,Risk Assessment: methods,Risk Factors,Social Support,dispersion,networks,review},
mendeley-tags = {dispersion,networks,review},
