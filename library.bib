@article{Lo2010,
abstract = {The quantitative aspirations of economists and financial analysts have for many years been based on the belief that it should be possible to build models of economic systems - and financial markets in particular - that are as predictive as those in physics. While this perspective has led to a number of important breakthroughs in economics, "physics envy" has also created a false sense of mathematical precision in some cases. We speculate on the origins of physics envy, and then describe an alternate perspective of economic behavior based on a new taxonomy of uncertainty. We illustrate the relevance of this taxonomy with two concrete examples: the classical harmonic oscillator with some new twists that make physics look more like economics, and a quantitative equity market-neutral strategy. We conclude by offering a new interpretation of tail events, proposing an "uncertainty checklist" with which our taxonomy can be implemented, and considering the role that quants played in the current financial crisis.},
archivePrefix = {arXiv},
arxivId = {1003.2688},
author = {Lo, Andrew W. and Mueller, Mark T.},
doi = {10.2139/ssrn.1563882},
eprint = {1003.2688},
file = {:home/jakob/Mendeley{\_}Papers/Lo, Mueller/Lo, Mueller - 2010 - WARNING Physics Envy May Be Hazardous To Your Wealth!.pdf:pdf},
issn = {1556-5068},
keywords = {efficient markets,financial crisis,history of economic,quantitative finance},
title = {{WARNING: Physics Envy May Be Hazardous To Your Wealth!}},
url = {http://arxiv.org/abs/1003.2688},
year = {2010}
}
@article{Seppelt2014,
abstract = {Many separate studies have estimated the year of peak, or maximum, rate of using an individual resource such as oil. However, no study has estimated the year of peak rate for multiple resources and investigated the relationships among them. We exploit time series on the appropriation of 27 global renewable and nonrenewable resources. We found 21 resources experienced a peak-rate year, and for 20 resources the peak-rate years occurred between 1960-2010, a narrow time window in the long human history. Whereas 4 of 7 nonrenewable resources show no peak-rate year, conversion to cropland and 18 of the 20 renewable resources have passed their peak rate of appropriation. To test the hypothesis that peak-rate years are synchronized, i.e., occur at approximately the same time, we analyzed 20 statistically independent time series of resources, of which 16 presented a peak-rate year centered on 2006 (1989-2008). We discuss potential causal mechanisms including change in demand, innovation and adaptation, interdependent use of resources, physical limitation, and simultaneous scarcity. The synchrony of peak-rate years of multiple resources poses a greater adaptation challenge for society than previously recognized, suggesting the need for a paradigm shift in resource use toward a sustainable path in the Anthropocene.},
author = {Seppelt, Ralf and Manceur, Ameur M and Liu, Jianguo and Fenichel, Eli P and Klotz, Stefan},
doi = {10.5751/ES-07039-190450},
file = {:home/jakob/Mendeley{\_}Papers/Seppelt et al/Seppelt et al. - 2014 - Synchronized peak-rate years of global resources use.pdf:pdf},
issn = {1708-3087},
journal = {Ecol. Soc.},
keywords = {limits to growth,peak-rate year,synchrony},
number = {4},
pages = {art50},
title = {{Synchronized peak-rate years of global resources use}},
url = {http://dx.doi.org/10.5751/ES-07039-190450{\%}5Cnhttp://www.ecologyandsociety.org/vol19/iss4/art50/},
volume = {19},
year = {2014}
}
