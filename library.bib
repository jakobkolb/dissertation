journal = {Proc. Natl. Acad. Sci.},
number = {30},
pages = {201703486},
pmid = {28696293},
title = {{Bayesian markets to elicit private information}},
url = {http://www.pnas.org/lookup/doi/10.1073/pnas.1703486114},
volume = {114},
year = {2017}
}
@article{Watts2007,
