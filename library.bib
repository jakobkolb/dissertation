@article{Vazquez2008a,
abstract = {We present a mathematical description of the voter model dynamics on heterogeneous networks. When the average degree of the graph is {\$}\backslashmu \backslashleq 2{\$} the system reaches complete order exponentially fast. For {\$}\backslashmu {\textgreater}2{\$}, a finite system falls, before it fully orders, in a quasistationary state in which the average density of active links (links between opposite-state nodes) in surviving runs is constant and equal to {\$}\backslashfrac{\{}(\backslashmu-2){\}}{\{}3(\backslashmu-1){\}}{\$}, while an infinite large system stays ad infinitum in a partially ordered stationary active state. The mean life time of the quasistationary state is proportional to the mean time to reach the fully ordered state {\$}T{\$}, which scales as {\$}T \backslashsim \backslashfrac{\{}(\backslashmu-1) \backslashmu{\^{}}2 N{\}}{\{}(\backslashmu-2) \backslashmu{\_}2{\}}{\$}, where {\$}N{\$} is the number of nodes of the network, and {\$}\backslashmu{\_}2{\$} is the second moment of the degree distribution. We find good agreement between these analytical results and numerical simulations on random networks with various degree distributions.},
archivePrefix = {arXiv},
arxivId = {0803.1686},
author = {Vazquez, F. and Eguiluz, V. M.},
doi = {10.1088/1367-2630/10/6/063011},
eprint = {0803.1686},
