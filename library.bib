@unpublished{MartinAdlera,
abstract = {We examine the interplay between changes in the functional distribution of income and the distribution of market income among households. We use micro data from the German Socio-Economic Panel as well as macro data from the German Federal Statistical Office from 2002 to 2008. We categorize and evaluate the implications of changes in the func- tional distribution of income upon the distribution of income among individuals on the basis of a simple theoretic framework that links the degree of the concentration of income from asset flows among individuals to the (structural) relationship between individuals' lev- els of market income and their respective income shares from asset flows. Our empirical analysis offers two insights: First, the relative rise of income from asset flows reported by German National Accounting Statistics is also evident in the micro data taken from the German Socio-Economic Panel. Second, rising capital income shares are associated with an increasing concentration of market income.},
author = {Adler, Martin and Schmid, Kai Daniel},
file = {:home/jakob/Mendeley{\_}Papers/Adler, Schmid/Adler, Schmid - 2012 - Shares and and Income Income Inequality Inequality.pdf:pdf},
title = {{Shares and and Income Income Inequality Inequality}},
year = {2012}
}
@article{Menck2013,
abstract = {The human brain, power grids, arrays of coupled lasers and the Amazon rainforest are all characterized by multistability. The likelihood that these systems will remain in the most desirable of their many stable states depends on their stability against significant perturbations, particularly in a state space populated by undesirable states. Here we claim that the traditional linearization-based approach to stability is too local to adequately assess how stable a state is. Instead, we quantify it in terms of basin stability, a new measure related to the volume of the basin of attraction. Basin stability is non-local, nonlinear and easily applicable, even to high-dimensional systems. It provides a long-sought-after explanation for the surprisingly regular topologies of neural networks and power grids, which have eluded theoretical description based solely on linear stability. We anticipate that basin stability will provide a powerful tool for complex systems studies, including the assessment of multistable climatic tipping elements.},
author = {Menck, Peter J. and Heitzig, Jobst and Marwan, Norbert and Kurths, J{\"{u}}rgen},
doi = {10.1038/nphys2516},
file = {:home/jakob/Mendeley{\_}Papers/Menck et al/Menck et al. - 2013 - How basin stability complements the linear-stability paradigm.pdf:pdf;:home/jakob/Mendeley{\_}Papers/Menck et al/Menck et al. - 2013 - How basin stability complements the linear-stability paradigm(2).pdf:pdf},
isbn = {1745-2473},
issn = {1745-2473},
journal = {Nat. Phys.},
number = {2},
pages = {89--92},
pmid = {84873406497},
title = {{How basin stability complements the linear-stability paradigm}},
url = {http://www.nature.com/nphys/journal/v9/n2/full/nphys2516.html{\%}5Cnhttp://www.nature.com/nphys/journal/v9/n2/pdf/nphys2516.pdf{\%}5Cnhttp://www.nature.com/doifinder/10.1038/nphys2516},
volume = {9},
year = {2013}
}
@article{Alfarano2008a,
